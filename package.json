{
  "name": "nybbl",
  "version": "1.0.0",
  "description": "Nybbl Ventures CLI",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "nybbl": "./bin/run.js"
  },
  "scripts": {
    "build": "rm -rf dist && tsc -b",
    "clean": "rm -rf dist",
    "prepack": "npm run build && oclif manifest"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@inquirer/prompts": "^4.0.0",
    "@oclif/core": "^3.1.2",
    "@oclif/plugin-help": "^6.0.1",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.3",
    "ora": "^5.4.1",
    "simple-git": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^18.19.10",
    "oclif": "^4.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "oclif": {
    "bin": "nybbl",
    "dirname": "nybbl",
    "commands": "./dist/commands",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}